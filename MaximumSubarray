
public class MaximumSubarray {

	public int maxSubArray(int[] nums) {
		int max = Integer.MIN_VALUE;
		for (int i = 0; i < nums.length; i++) {
			int total = nums[i];
			if (total > max) {
				max = total;
			}
			for (int j = i + 1; j < nums.length; j++) {
				total += nums[j];
				if (total > max) {
					max = total;
				}
			}
		}
		return max;
	}
}
